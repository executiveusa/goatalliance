{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["expo", "react", "react-native"]
  },
  "extends": "expo/tsconfig.base",
  "include": ["./src", "./App.tsx"],
  "exclude": ["node_modules"]
}
